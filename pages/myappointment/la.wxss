/* pages/my appointment/la.wxss */
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1690399_dc393tpqfy.eot?t=1585668136670'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1690399_dc393tpqfy.eot?t=1585668136670#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1690399_dc393tpqfy.woff?t=1585668136670') format('woff'),
  url('//at.alicdn.com/t/font_1690399_dc393tpqfy.ttf?t=1585668136670') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1690399_dc393tpqfy.svg?t=1585668136670#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-ico_meiyoushuju:before {
  content: "\e602";
}
page{
  height: 100%;
  background-color:rgb(243, 243, 243); 
}
.container{
  display: flex;
  flex-direction:column;
  align-items: center;
  margin-bottom: 40rpx;
}
.card {
  width: 650rpx;
  background-color: #fff;
  margin: 40rpx auto 0;
  border-radius: 20rpx;
  box-shadow: 10rpx 10rpx 20rpx rgb(206, 204, 204);
}
.card text{
  width:50%;
  padding: 15rpx 90rpx 0rpx; 
}
.txt{
  color:black;
  padding:20rpx 20rpx; 
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
  background-color: rgb(214, 223, 253);
}
.agree{
  background-color: #94eb92;
}
.disagree{
  background-color: #fc5d5d;
}
.finish{
  background-color: #d0d1b5;
}
.txt text{
  display: flex;
  padding:10rpx 0 0; 
  font-size: 25rpx;
  width: 400rpx;
}
.itm{
  padding:7rpx 5rpx 10rpx;
  font-size:25rpx;
}
text{
  color: rgb(136, 134, 134);
  font-size: 30rpx;
  }

.down {
  background-color: #fffdfd;
  border-bottom-left-radius: 20px;
  border-bottom-right-radius: 20rpx;
  width: 650rpx;
  box-shadow: 10rpx 10rpx 20rpx rgb(206, 204, 204);
}

.down view {
  color: rgb(0, 0, 0);
  text-overflow:ellipsis;
  word-wrap:break-word;
  font-size: 27rpx;
  padding: 0 80rpx ;
}
.down text{
  
  font-size: 25rpx;
  padding: 7rpx 10rpx;

}

.feedBack1 {
  float: left;
  text-align: center;
  width:50%;
  background-color: #fcdfcf;
  border-bottom-left-radius: 10px;
  padding: 5rpx 0;
  margin: 10rpx 0 0 0;
  font-size: 30rpx;
}
.feedBack2 {
  float: right;
  text-align: center;
  width:50%;
  background-color: rgb(214, 223, 253);
  border-bottom-right-radius: 10rpx;
  padding: 5rpx 0;
  margin: 10rpx 0 0 0;
  font-size: 30rpx;
}
.feedBack3 {
  text-align: center;
  background-color: rgb(120, 173, 218);
  border-bottom-right-radius: 10rpx;
  padding: 5rpx 0;
  width: 650rpx;
  margin: 10rpx 0 0 0;
  font-size: 30rpx;
}
.hidden{
  display:none;
}


.clearfix {
  zoom: 1;
  clear: both;
}

.clearfix:after {
  display: block;
  height: 0;
  overflow: hidden;
  content: " ";
  visibility: hidden;
  clear: both;
}
.icon{
  background-color: rgb(243, 243, 243);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 60vh;
}
.icon-ico_meiyoushuju:before{
  color: rgb(216, 215, 215);
  font-size: 250rpx;
}
.content_text{
  color: rgb(216, 215, 215);
  font-size: 40rpx;
}